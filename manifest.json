{
  "content_scripts": [
    {
      "js": ["js/script.js"],
      "matches": ["https://github.com/*"],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "description": "GitHubで名前にエイリアスを設定するための拡張です",
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "manifest_version": 3,
  "name": "GitHubで名前にエイリアスを設定するやつ",
  "options_page": "options.html",
  "version": "0.2",
  "permissions": ["storage"]
}
